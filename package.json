{
  "name": "alfdocimo-dev",
  "version": "0.0.1",
  "description": "Personal portfolio developed with React and Redux, firebase, cypress and react testing library",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "main": "./dist",
  "scripts": {
    "start": "concurrently -k -r -s first \"yarn open:src\" \"yarn lint:watch\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "gh-pages-deploy": "babel-node tools/deployGithubPages.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "yarn lint -- --watch",
    "clean-dist": "yarn remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "yarn lint && yarn clean-dist",
    "build": "babel-node tools/build.js && yarn open:dist",
    "test": "jest",
    "test:CI": "jest --forceExit",
    "test:cover": "yarn test -- --coverage ",
    "test:cover:CI": "yarn test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watchAll",
    "open:cover": "yarn test:cover && opn ./coverage/lcov-report/index.html",
    "cypress:open": "cypress open",
    "e2e-test": "cypress run",
    "e2e-test:record": "cypress run --record --key 379af14b-28c4-40fb-8c9c-cf5e080095ac",
    "test:all:concurrent": "concurrently -k -r -s first \"yarn start\" \"yarn e2e-test:record\"",
    "heroku-prebuild": "",
    "heroku-postbuild": "babel-node tools/build.js"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "@hot-loader/react-dom": "16.8.6",
    "antd": "latest",
    "connected-react-router": "6.4.0",
    "express": "^4.17.1",
    "firebase": "7.4.0",
    "heroku": "^7.35.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "autoprefixer": "9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "browser-sync": "2.26.7",
    "chalk": "2.4.2",
    "concurrently": "4.1.0",
    "connect-history-api-fallback": "1.6.0",
    "coveralls": "3.0.3",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "cypress": "3.6.1",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-react": "7.13.0",
    "eslint-watch": "5.1.2",
    "file-loader": "3.0.1",
    "gh-pages": "2.1.1",
    "hard-source-webpack-plugin": "0.13.1",
    "history": "4.9.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "json-loader": "0.5.7",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "mini-css-extract-plugin": "0.6.0",
    "mockdate": "2.0.2",
    "node-sass": "4.12.0",
    "opn-cli": "4.1.0",
    "postcss-loader": "3.0.0",
    "prompt": "1.0.0",
    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react-hot-loader": "4.8.4",
    "react-test-renderer": "16.8.6",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "replace": "1.1.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.31.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-dev-middleware": "3.6.2",
    "webpack-hot-middleware": "2.24.4"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "reload",
    "webpack",
    "redux",
    "firebase",
    "alfdocimo",
    "portfolio",
    "cypress"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alfdocimo/alfdocimo-dev"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/cypress/integration"
    ],
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties",
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "globals": {}
  }
}
